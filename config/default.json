{
    "guardhouse": {
        "token_client": "",
        "token_server": "",
        "server": "https://agent.guardhou.se",
        "allow_unsafe": false,
        "poll_interval_seconds": 60
    },
    "server": {
        "enabled": true,
        "port": "3012"
